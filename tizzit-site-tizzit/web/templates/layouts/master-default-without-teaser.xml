							<div id="contentcontainer">
								<div id="leftmenue">
									<ctmpl:include name="leftmenue"/>
								</div>
								<div id="middle">
									<ctmpl:include name="headline"/> 
									<ctmpl:include name="content"/>
									<ctmpl:include name="tagging"/>
								</div>
								<div class="clear">&#160;</div>
							</div>
						</div>
						<div id="information_2">
							<ctmpl:include name="teaser_bottom"/>
