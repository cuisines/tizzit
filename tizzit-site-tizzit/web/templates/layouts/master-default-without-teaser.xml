<?xml version="1.0"?>
<html xmlns:ctmpl="http://www.conquest-cms.net/template">
	<head>
		<ctmpl:include name="meta"/>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<ctmpl:include name="css"/>
		<link rel="alternate" type="application/rss+xml" title="RSS-Feed" href="content.rss" />
		<script language="JavaScript" src="/jscript/scripts.js" type="text/JavaScript"><![CDATA[&#160;]]></script>
		<script language="JavaScript" type="text/JavaScript" src="/httpd/js/adapter/shadowbox-base.js"><![CDATA[ ]]></script>
		<script language="JavaScript" type="text/JavaScript" src="/httpd/js/shadowbox.js"><![CDATA[ ]]></script>
		<script language="JavaScript" type="text/javaScript">
			<![CDATA[
				Shadowbox.loadSkin('classic', '/httpd/js/skin');
				Shadowbox.loadLanguage('de-DE', '/httpd/js/lang');
				Shadowbox.loadPlayer(['flv','html','iframe','img','swf'], '/httpd/js/player');
				window.onload = function(){
					Shadowbox.init();
				};
			]]>
		</script>
	</head>
