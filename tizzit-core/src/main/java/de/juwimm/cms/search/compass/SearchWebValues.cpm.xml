<?xml version="1.0"?>
<!DOCTYPE compass-core-mapping PUBLIC
    "-//Compass/Compass Core Mapping DTD 2.0//EN"
    "http://www.compass-project.org/dtd/compass-core-mapping-2.1.dtd">    
<compass-core-mapping package="de.juwimm.cms.search.vo">
    <resource alias="HtmlSearchValue">
        <resource-id name="uid" />
        <resource-property name="url" store="yes" index="un_tokenized" />
        <resource-property name="unitId" store="yes" index="un_tokenized" />
        <resource-property name="siteId" store="yes" index="un_tokenized" />
        <resource-property name="unitName" store="yes" index="un_tokenized" />
        <resource-property name="isLiveContent" store="yes" index="un_tokenized" />
        <resource-property name="language" store="yes" index="un_tokenized" />
        <resource-property name="viewtype" store="yes" index="un_tokenized" />
        <resource-property name="template" store="yes" index="un_tokenized" />
        <resource-property name="modified" store="yes" index="un_tokenized" />
        <resource-property name="realm" store="yes" index="tokenized" />
		<resource-property name="viewComponentId" store="yes" index="un_tokenized" />
        <resource-property name="contents" store="compress" index="tokenized" />
        <resource-property name="metadata" store="yes" index="tokenized" />
        <resource-property name="summary" store="yes" index="no" />
        <resource-property name="title" store="yes" index="tokenized" />
    </resource>
</compass-core-mapping>
